{
  "name": "hdvinnie/laravel-security-headers",
  "type": "library",
  "description": "Adds security related headers to HTTP response.",
  "keywords": [
    "laravel",
    "header",
    "https",
    "hsts",
    "csp",
    "except-ct",
    "feature-policy",
    "clear-site-data",
    "referrer-policy",
    "content-security-policy",
    "HDVinnie"
  ],
  "homepage": "https://github.com/HDVinnie/laravel-security-headers",
  "license": "MIT",
  "authors": [
    {
      "name": "HDVinnie",
      "email": "hdinnovations@protonmail.com",
      "homepage": "https://github.com/HDVinnie",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^6|^7|^8|^9"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-xdebug": "*",
    "orchestra/testbench": "^3.1|^4.0|^5.0|^6.0"
  },
  "suggest": {
    "friendsofphp/php-cs-fixer": "Coding style fixer",
    "phpstan/phpstan": "Static analysis tool",
    "phpunit/phpunit": "PHP unit testing framework"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HDVinnie\\SecureHeaders\\SecureHeadersServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "HDVinnie\\SecureHeaders\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HDVinnie\\SecureHeaders\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
